<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#44cccccc"
    android:orientation="vertical" >
	
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#50A9E9">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:paddingBottom="10dip"
            android:paddingTop="10dip"
            android:text="@string/set_up"
            style="@style/font_color" />

        

        
      
    </RelativeLayout>

  
	<View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"
	    android:layout_marginTop="5dip"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/account_manager_zhanghao"
        android:orientation="horizontal" 
        android:onClick="cc"
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/account_manager1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zhanghaoguanli" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />

        <ImageButton
            android:id="@+id/user_manager_touxiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/imageView1"
            android:background="@null"
            android:src="@drawable/user_manager_touxiang" />
        
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#fff"
        android:orientation="horizontal"
        android:padding="10dip" >

        <TextView
            android:id="@+id/message_manager"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dip"
            android:paddingBottom="5dip"
            android:paddingTop="5dip"
            android:text="@string/xiaoxiguanli" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />
    </RelativeLayout>

    <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"/>
    <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"/>
    
     <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"
	    android:layout_marginTop="10dip"/>
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" 
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/chat_histioy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/chathistry" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />
        
    </RelativeLayout>
    
       <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"/>
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" 
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/position_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/weizhifuwu" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <CheckBox
            android:id="@+id/start_and_close_posit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/start_posit_and_close_posit"
            android:button="@null"/>
        
    </RelativeLayout>
    
       <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"
	    android:layout_marginTop="10dip"/>
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" 
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/pessims_hide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/quanxianyinsi" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />
        
    </RelativeLayout>
    
       <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc"
	   />
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" 
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/message_cast"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/info" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />
        
    </RelativeLayout>
    
       
       <View 
	    android:layout_width="fill_parent"
	    android:layout_height="1dip"
	    android:background="#cccccc" />
    
 
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" 
        android:background="#fff"
        android:padding="10dip">

        <TextView
            android:id="@+id/about_us"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/asforour" 
            android:paddingTop="5dip"
			android:paddingBottom="5dip"
            android:layout_marginLeft="5dip"/>

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/boss_unipay_arrow_right_normal" />
        
    </RelativeLayout>

</LinearLayout>